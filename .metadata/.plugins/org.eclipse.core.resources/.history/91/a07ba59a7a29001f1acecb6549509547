package singleinhritence;

public class Area{
	public static int multiply(int a, int b) {
		return a*b;
		
		
	}
	public static void main(String[] args) {
		int a = 10;
		int b= 15;
		int r=5;
		int c= multiply(a, b);
		int d= (int) (3.14*r*r) ;
		int e = (a*b*r);
		System.out.println("area of rectangle is  "+c);
		System.out.println("area of circle is "+d);
		System.out.println("area of triangle is "+ e);
	}
}

		
	
	
	
	
		

